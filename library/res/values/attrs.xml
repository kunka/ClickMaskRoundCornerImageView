<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundCornerImageView">
        <attr name="radiusX" format="dimension" />
        <attr name="radiusY" format="dimension" />
        <attr name="radiusXY" format="dimension" />
        <attr name="background" format="color" />
        <attr name="isRound" format="boolean" />
